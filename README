  /usr/share/doc/tidy-20080811/html/pending.html
  /usr/share/doc/tidy-20080811/html/quickref-html.xsl
  /usr/share/doc/tidy-20080811/html/release-notes.html
  /usr/share/doc/tidy-20080811/html/tidy.css
  /usr/share/doc/tidy-20080811/html/tidy.gif
  /usr/share/doc/tidy-20080811/html/tidy1.xsl

(libtidy0_99_0)
  /usr/bin/cygtidy-0-99-0.dll

(libtidy-devel)
  /usr/include/tidy/buffio.h
